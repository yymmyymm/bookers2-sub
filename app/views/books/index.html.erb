<%= render 'books/list', user: @user, book: @book %>



<h1>Books</h1>

    <% @books.each do |book| %>

  <div>


    <%= link_to user_path(@user.id) do %>
      <p><%= image_tag @user.get_profile_image(100,100) %></p>
    <% end %>

    <%= link_to book_path(book.id) do %>
      <p>Title：<%= book.title %></p>
    <% end %>


      <p>Option：<%= book.body %></p>

  <% end %>


  </div>
